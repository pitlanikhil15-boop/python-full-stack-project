    "%%writefile student_manager.py\n",
    "import streamlit as st\n",
    "from supabase import create_client\n",
    "import pandas as pd\n",
    "\n",
    "url = \"https://dzzdkfyoghncusvtoaaa.supabase.co\"\n",
    "key = \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImR6emRrZnlvZ2huY3VzdnRvYWFhIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTgyNTY2MzQsImV4cCI6MjA3MzgzMjYzNH0.SxSpzlreqyLJunTpXbsBazYDJXQKc3cknHhmJjmLRdc\" \n",
    " \n",
    "supabase = create_client(url, key)\n",
    "\n",
    "st.set_page_config(page_title=\"Student Manager\", page_icon=\"üéì\", layout=\"centered\")\n",
    "\n",
    "st.title(\"üéì Student Manager with Supabase\")\n",
    "st.write(\"Add students, check attendance & eligibility\")\n",
    "\n",
    "with st.form(\"student_form\"):\n",
    "    name = st.text_input(\"Name\")\n",
    "    age = st.number_input(\"Age\", min_value=1, max_value=100, step=1)\n",
    "    attendance = st.number_input(\"Attendance (%)\", min_value=0.0, max_value=100.0, step=0.5)\n",
    "    submitted = st.form_submit_button(\"‚ûï Add Student\")\n",
    "    \n",
    "    if submitted and name:\n",
    "        supabase.table(\"students\").insert({\"name\": name, \"age\": age, \"attendance\": attendance}).execute()\n",
    "        st.success(f\"‚úÖ {name} added successfully!\")\n",
    "\n",
    "st.subheader(\"üìã Student List\")\n",
    "res = supabase.table(\"students\").select(\"*\").execute()\n",
    "data = res.data\n",
    "\n",
    "if data:\n",
    "    df = pd.DataFrame(data)\n",
    "    df[\"Eligibility\"] = df[\"attendance\"].apply(lambda x: \"‚úÖ Eligible\" if x >= 75 else \"‚ùå Not Eligible\")\n",
    "    st.dataframe(df, use_container_width=True)\n",
    "else:\n",
    "    st.info(\"No students found. Add some!\")\n"
   ]
